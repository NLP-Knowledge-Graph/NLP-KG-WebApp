    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Model_Stealing_Attack -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Model_Stealing_Attack">
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Task"/>
