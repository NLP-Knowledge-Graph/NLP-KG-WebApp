        <nlp-taxonomy:subFieldOf rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#Text_Classification"/>
        <survey>https://aclanthology.org/2020.coling-main.208</survey>
        <survey>https://aclanthology.org/2023.ranlp-stud.1</survey>
        <survey>https://arxiv.org/abs/2210.06336</survey>
        <survey>https://arxiv.org/abs/2210.07321</survey>
        <survey>https://arxiv.org/abs/2303.07205</survey>
        <survey>https://arxiv.org/abs/2304.04736</survey>
        <survey>https://arxiv.org/abs/2310.05030</survey>
        <survey>https://arxiv.org/abs/2310.14724</survey>
        <nlp-taxonomy:level rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</nlp-taxonomy:level>
        <dcterms:description xml:lang="en">Machine Generated Text Detection refers to the process of identifying and distinguishing text that has been generated by an artificial intelligence or machine learning model, as opposed to text that has been written by a human.</dcterms:description>
