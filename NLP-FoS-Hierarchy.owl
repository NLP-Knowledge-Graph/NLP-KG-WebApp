        <dcterms:description xml:lang="en">Extractive Summarization involves automatically selecting and extracting key sentences or phrases from a text document to create a concise summary. Extractive Summarization identifies and reuses the most informative and relevant content from the original text to produce a summary that captures the main ideas and important details.</dcterms:description>
        <oboInOwl:hasSynonym xml:lang="en">Extractive Summarisation</oboInOwl:hasSynonym>
        <rdfs:label xml:lang="en">Extractive Summarization</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Fact_Discourse_Deixis_Resolution -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Fact_Discourse_Deixis_Resolution">
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Task"/>
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#FieldOfStudy"/>
