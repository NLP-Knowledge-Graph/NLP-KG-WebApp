        <dcterms:description xml:lang="en">Event Detection refers to the process of identifying and categorizing specific events or occurrences within a text. These instances can be anything from a simple action, such as a transaction, to a complex incident, like a natural disaster.</dcterms:description>
        <oboInOwl:hasSynonym xml:lang="en">Event Classification</oboInOwl:hasSynonym>
        <oboInOwl:hasSynonym xml:lang="en">Event Identification</oboInOwl:hasSynonym>
        <rdfs:label xml:lang="en">Event Detection</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Event_Discourse_Deixis_Resolution -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Event_Discourse_Deixis_Resolution">
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Task"/>
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#FieldOfStudy"/>
