    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Federated_Learning -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Federated_Learning">
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Task"/>
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#FieldOfStudy"/>
