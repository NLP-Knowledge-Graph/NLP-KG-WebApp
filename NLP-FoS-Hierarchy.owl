        <nlp-taxonomy:level rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</nlp-taxonomy:level>
        <dcterms:description xml:lang="en">Morphemes refer to the smallest meaningful units of a language. They are the building blocks of words, which can&apos;t be further divided without losing or changing their meaning. For example, the word &quot;unhappiness&quot; contains three morphemes: &quot;un-&quot;, &quot;happy&quot;, and &quot;-ness&quot;.</dcterms:description>
        <rdfs:label xml:lang="en">Morphemes</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Morphological_Inflection -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Morphological_Inflection">
