    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Model_Inversion_Attack -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Model_Inversion_Attack">
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Task"/>
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#FieldOfStudy"/>
