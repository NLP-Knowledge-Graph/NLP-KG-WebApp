    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Language_Model_Quantization -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Language_Model_Quantization">
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Task"/>
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#FieldOfStudy"/>
        <nlp-taxonomy:subFieldOf rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#Language_Models"/>
        <nlp-taxonomy:subFieldOf rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#Green_&amp;_Sustainable_NLP"/>
        <nlp-taxonomy:superFieldOf rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Post-Training_Quantization"/>
        <nlp-taxonomy:superFieldOf rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Quantization_Aware_Training"/>
