    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Abstract_Meaning_Representation -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Abstract_Meaning_Representation">
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#DataStructure"/>
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#FieldOfStudy"/>
        <nlp-taxonomy:subFieldOf rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Graphs"/>
        <nlp-taxonomy:subFieldOf rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Text_Representations"/>
        <nlp-taxonomy:superFieldOf rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#AMR-to-Text"/>
        <relevantInformation>https://web.stanford.edu/~jurafsky/slp3/19.pdf</relevantInformation>
        <survey>https://arxiv.org/abs/2206.07328</survey>
        <nlp-taxonomy:level rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</nlp-taxonomy:level>
        <nlp-taxonomy:level rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</nlp-taxonomy:level>
        <dcterms:description xml:lang="en">Abstract Meaning Representation (AMR) is a semantic representation language that encodes meanings of sentences in a structured and interpretable form. It represents sentences as rooted, directed, acyclic graphs that capture core semantic information, such as who is doing what to whom, along with additional details like modality, tense, and polarity.</dcterms:description>
        <oboInOwl:hasSynonym xml:lang="en">AMR</oboInOwl:hasSynonym>
        <oboInOwl:hasSynonym xml:lang="en">Meaning Representation</oboInOwl:hasSynonym>
        <rdfs:label xml:lang="en">Abstract Meaning Representation</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Abstractive_Summarization -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Abstractive_Summarization">
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Task"/>
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#FieldOfStudy"/>
        <nlp-taxonomy:subFieldOf rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#Summarization"/>
        <survey>https://arxiv.org/abs/1812.02303</survey>
        <survey>https://arxiv.org/abs/2104.14839</survey>
        <survey>https://arxiv.org/abs/2208.04163</survey>
        <survey>https://arxiv.org/abs/2303.17650</survey>
        <nlp-taxonomy:level rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</nlp-taxonomy:level>
        <dcterms:description xml:lang="en">Abstractive Summarization involves generating a concise and coherent summary of a longer text by interpreting and rephrasing the content in a more abstract or human-like manner. Abstractive Summarization aims to produce concise, coherent, and informative summaries by generating new sentences or phrases that capture the essence of the source material. This approach allows for flexibility in summarizing content but can be challenging to ensure the generated summaries are both accurate and coherent.</dcterms:description>
        <oboInOwl:hasSynonym xml:lang="en">Abstractive Summarisation</oboInOwl:hasSynonym>
        <oboInOwl:hasSynonym xml:lang="en">Generative Summarisation</oboInOwl:hasSynonym>
        <oboInOwl:hasSynonym xml:lang="en">Generative Summarization</oboInOwl:hasSynonym>
        <rdfs:label xml:lang="en">Abstractive Summarization</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Acoustic_Modeling -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Acoustic_Modeling">
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Task"/>
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#FieldOfStudy"/>
        <nlp-taxonomy:subFieldOf rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#Speech_Recognition"/>
        <nlp-taxonomy:level rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</nlp-taxonomy:level>
        <dcterms:description xml:lang="en">Acoustic Modeling involves the process of modeling and analyzing the acoustic properties of spoken language, specifically the relationship between the acoustic signal (i.e., the audio waveform) and the phonemes or other linguistic content of speech.</dcterms:description>
        <rdfs:label xml:lang="en">Acoustic Modeling</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Acoustic_Signal_Processing -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Acoustic_Signal_Processing">
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Task"/>
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#FieldOfStudy"/>
