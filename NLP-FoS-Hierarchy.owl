        <nlp-taxonomy:level rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</nlp-taxonomy:level>
        <dcterms:description xml:lang="en">MVP (Multi-task superVised Pre-training) is a language model that is specially designed for natural language generation and can be adapted to a wide range of generation tasks, including but not limited to summarization, data-to-text generation, open-ended dialogue system, story generation, question answering, question generation, task-oriented dialogue system, commonsense generation, paraphrase generation, text style transfer, and text simplification. The model can also be adapted to natural language understanding tasks such as sequence classification and (extractive) question answering.</dcterms:description>
        <oboInOwl:hasSynonym xml:lang="en">Multi-task superVised Pre-training</oboInOwl:hasSynonym>
        <rdfs:label xml:lang="en">MVP</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Machine_Generated_Text_Detection -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Machine_Generated_Text_Detection">
