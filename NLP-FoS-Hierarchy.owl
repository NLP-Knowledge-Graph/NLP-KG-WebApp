    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Explicit_Discourse_Relation_Recognition -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Explicit_Discourse_Relation_Recognition">
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Task"/>
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#FieldOfStudy"/>
        <nlp-taxonomy:subFieldOf rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Discourse_Relation_Recognition"/>
