    </owl:NamedIndividual>
    


    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Long-Range_Transformers -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Long-Range_Transformers">
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#OtherScientificField"/>
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#FieldOfStudy"/>
        <nlp-taxonomy:subFieldOf rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Efficient_Transformers"/>
        <survey>https://aclanthology.org/2023.eacl-main.273</survey>
        <survey>https://arxiv.org/abs/2302.14502</survey>
        <nlp-taxonomy:level rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</nlp-taxonomy:level>
        <dcterms:description xml:lang="en">Long-Range Transformers refer to a type of transformer model that is designed to handle long sequences of data more efficiently. Traditional transformer models struggle with long sequences due to their quadratic complexity with respect to the sequence length. Long-Range Transformers address this issue by using methods such as sparse attention mechanisms or other techniques to reduce the computational complexity, enabling the model to process longer sequences of text while maintaining high performance.</dcterms:description>
        <rdfs:label xml:lang="en">Long-Range Transformers</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#MVP -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#MVP">
