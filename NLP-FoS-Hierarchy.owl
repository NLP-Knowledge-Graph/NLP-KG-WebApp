    </owl:NamedIndividual>
    


    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Active_Learning -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Active_Learning">
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Task"/>
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#FieldOfStudy"/>
        <nlp-taxonomy:subFieldOf rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#Low-Resource_NLP"/>
        <nlp-taxonomy:subFieldOf rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#Green_&amp;_Sustainable_NLP"/>
        <nlp-taxonomy:superFieldOf rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Proactive_Learning"/>
        <survey>https://aclanthology.org/2022.emnlp-main.414</survey>
        <survey>https://arxiv.org/abs/2008.07267</survey>
        <nlp-taxonomy:level rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</nlp-taxonomy:level>
        <dcterms:description xml:lang="en">Active Learning is a machine learning approach where the model selects the data from which it learns. The model queries a user (an oracle who can provide the correct output) to label new data based on what it thinks will improve its learning the most. This approach is particularly useful when there is a lot of unlabeled data and labeling it all would be too time-consuming or expensive.</dcterms:description>
        <rdfs:label xml:lang="en">Active Learning</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Adversarial_Attacks -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Adversarial_Attacks">
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Task"/>
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#FieldOfStudy"/>
        <nlp-taxonomy:subFieldOf rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Privacy,_Security_&amp;_Safety_in_NLP"/>
        <nlp-taxonomy:superFieldOf rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Adversarial_Defenses"/>
        <code>https://github.com/QData/TextAttack</code>
        <survey>https://arxiv.org/abs/1901.06796</survey>
        <survey>https://arxiv.org/abs/2005.14108</survey>
        <survey>https://arxiv.org/abs/2103.00676</survey>
        <survey>https://arxiv.org/abs/2310.10844</survey>
        <nlp-taxonomy:level rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</nlp-taxonomy:level>
        <dcterms:description xml:lang="en">Adversarial attacks refer to the manipulation of input data with malicious intent to deceive NLP systems. This is typically done by introducing small, often imperceptible alterations to the original data, which can lead to significant changes in the system&apos;s output. The goal of these attacks is to exploit the vulnerabilities of the NLP models and cause them to make errors or produce incorrect results.</dcterms:description>
        <rdfs:label xml:lang="en">Adversarial Attacks</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Adversarial_Defenses -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Adversarial_Defenses">
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Task"/>
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-taxonomy.owl#FieldOfStudy"/>
        <uses rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Adversarial_Training"/>
        <nlp-taxonomy:subFieldOf rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Adversarial_Attacks"/>
