        <nlp-taxonomy:level rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</nlp-taxonomy:level>
        <dcterms:description xml:lang="en">Event Coreference Resolution refers to the task of determining when two expressions in a text refer to the same event. This is crucial for understanding the context and semantics of a text, as it helps in linking different parts of a text that are talking about the same event.</dcterms:description>
        <oboInOwl:hasSynonym xml:lang="en">ECR</oboInOwl:hasSynonym>
        <rdfs:label xml:lang="en">Event Coreference Resolution</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Event_Detection -->

    <owl:NamedIndividual rdf:about="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Event_Detection">
        <rdf:type rdf:resource="http://nlpkg.sebis.cit.tum.de/nlp-fos-hierarchy.owl#Task"/>
